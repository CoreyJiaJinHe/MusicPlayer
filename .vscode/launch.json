{
  "$schema": "https://raw.githubusercontent.com/microsoft/vscode-python/main/pythonFiles/vscode_pytest/launcher.schema.json",
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run MusicPlayer",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\main.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "envFile": "${workspaceFolder}\\.env",
      "env": {
        "QTWEBENGINE_CHROMIUM_FLAGS": "--disable-direct-composition"
      }
    }
  ]
}
